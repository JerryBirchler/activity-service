{
  "server": {
    "port": 8181,
    "prefix": "/activity-service"
  },
  "log4js": {
    "appenders": {
      "console": {
        "type": "console",
        "layout": {
          "type": "basic"
        }
      }
    },
    "categories": {
      "default": {
        "appenders": ["console"],
        "level": "debug"
      }
    }
  },
  "required_request_properties": ["entity.application", "entity.type", "entity.name", "task.type", "current"]
}
