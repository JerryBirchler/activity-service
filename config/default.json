{
  "server": {
    "port": 8081,
    "prefix": "/activity-service"
  },
  "log4js": {
    "appenders": {
      "console": {
        "type": "console",
        "layout": {
          "type": "basic"
        }
      }
    },
    "categories": {
      "default": {
        "appenders": ["console"],
        "level": "debug"
      }
    }
  },
  "request_identity": { "entity": "application,type,name" },
  "request_task": { "task": "type" }
}
